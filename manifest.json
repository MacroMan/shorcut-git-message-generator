{
  "manifest_version": 3,
  "name": "Git commit message generator for shortcut",
  "version": "1.0",
  "description": "Adds a Git helper field in Shortcut with a generated commit message",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["src/index.js"],
      "matches": [
        "https://app.shortcut.com/*"
      ]
    }
  ]
}
